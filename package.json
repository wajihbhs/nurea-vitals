{
  "name": "nurea-vitals",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "mock:api": "json-server --watch mock/db.json --port 3333",
    "lint": "eslint --ext .ts,.tsx,.js,.vue src",
    "format": "prettier --write \"src/**/*.{ts,js,vue,json,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "prepare": "husky install"
  },
  "dependencies": {
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@testing-library/vue": "^8.1.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/ui": "^3.2.4",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.4.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "json-server": "^1.0.0-beta.3",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.92.1",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.5"
  },
  "lint-staged": {
    "*.{ts,js,vue}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
